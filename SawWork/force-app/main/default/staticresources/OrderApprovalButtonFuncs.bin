function CreateOrderApproval(pOppId, pOppRecTypeId, pOppStageName , pGetField) 
{
	var result = "";

	try
	{
		result = sforce.apex.execute("OrderApprovalClass","Create_OrderApproval",
											{
												p_OppId:pOppId,
												p_RecTypeId:pOppRecTypeId,
												p_Stage:pOppStageName,
												p_GetField:pGetField
											}
					     ) + "";
	}
	catch(err)
	{
		var myRegExp = /INSUFFICIENT_ACCESS/;
		var errString = err+"";
		console.log("OrderApprovalButton Error:"+err.message);
		if(errString.search(myRegExp) != -1)
			result = "0:You do not have sufficient access to view an Order Approval.";
		else
			result = "0:An unexpected error has occurred. Please contact your System Administrator\n\n" + "Error : " + err;
	}

	return result;
}


