<aura:component >
    <aura:attribute name="baseIterationValue" type="String" default="5" access="PRIVATE"/>
    <aura:attribute name="value" type="String" default="5"/>    
    <aura:attribute name="title" type="String" default="How would you rate our service ?"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.starInitAction}" />
	<div class="slds-form-element">
        <label class="slds-form-element__label" for="">
            <span class="slds-slider-label">
                <span class="slds-form-element__legend slds-slider-label__label">{!v.title}</span>
            </span>
        </label>
        <div style="display:block-inline">
         {!v.body}
         </div>
    </div>
</aura:component>