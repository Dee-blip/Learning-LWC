<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,force:hasRecordId" access="global"
    controller="HD_IncidentDetailController">
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="instanceURL" type="String" />
    <aura:attribute name="renderPrintComponent" type="Boolean" />
    <aura:handler name="init" action="{!c.doInit}" value="{!this}" />
    <lightning:messageChannel type="hdQuickActionClick__c" onMessage="{!c.print}" />
    <div>
        <div class="slds-form-element">
            <ui:button aura:id="PrintTicket" label="Print Ticket" press="{!c.print}" />
        </div>
    </div>
</aura:component>