<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId"
	access="global" controller="HD_PageAnalytics">
	<!-- 
	@Author: Nikhil Karn
	Created Date:30th Oct,2017
	Details: This component is to track and audit the user who is visiting particular page.
	
	 -->
	<aura:attribute name="incident" type="Id" />
    <aura:attribute name="pageVisitTime" type="DateTime" />
    <aura:attribute name="pageName" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.fireMe}" />
</aura:component>