<aura:component controller="communityLoginLinkController" implements="forceCommunity:availableForAllPageTypes,force:lightningQuickAction">
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <div class="communityLoginLink" 
         style="width: 100%;
                background: rgb(17, 59, 79);
                width: auto;
                margin-top: 0.7%;
                height: 70px;
                ">
        
        <div class="topLeftBrandImgLogo">	
            <!--<span style="text-align: left;margin-left: 1%;cursor:pointer" onclick="{!c.goToHome}">-->
                <img onclick="{!c.goToHome}" src="/customers/file-asset/Originalakamailogo2?v=1" class="akamaiCommLogoBarCls" style="
    margin-top: 5px;
    text-align: left;
    margin-left: 3%;
    cursor: pointer;
    min-width: 9%;
    width: 134.0px;
    height: 54.0px;"/>
            
            <div class="topLeftBrandTextLogo">
                <span class="logo-text-part">
                    {!$Label.c.Jarvis_communityLoginLinks_CommunityHelpLink} <!--Community-->
                </span>
                
                <span class="logo-text-part2">
                </span>
            </div>
            
            
        </div>
        <div class="topRightLinks">
            <span class="headerLink" onclick="{!c.openLearnAkamai}">{!$Label.c.Jarvis_communityLoginLinks_LearnAkamaiLink}</span> <!--Learn Akamai-->
            <span class="headerLink" onclick="{!c.openAkamaidotCom}">Akamai.com</span>
            <span class="headerLink" onclick="{!c.openCommunityHelp}">{!$Label.c.Jarvis_communityLoginLinks_CommunityHelpLink}</span> <!--Community Help-->        
        </div>
    </div>
    
</aura:component>