<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction" access="global" >
    <aura:attribute name="listName" type="String"/>
    <aura:attribute name="sObjectName" type="String"/> 
    
    <div class="sectionFour parentContainer">
        <div class="slds-media__body">
            <div class="slds-tile__detail slds-text-body--small">
              	<dl class=" slds-box slds-list--horizontal slds-wrap" style="height:3.00rem;" onclick="{!c.selectList}">
                     <dt>
                         <lightning:icon iconName="utility:list" size="small" alternativeText="icon"></lightning:icon>
                     </dt>
                     <dd aura:id="itemSelected" class="slds-item--detail slds-truncate" style="margin-left:2vw;margin-top:0.8vw;">{!v.listName}</dd>
               	</dl>	
            </div>
        </div>
        
    </div>
</aura:component>