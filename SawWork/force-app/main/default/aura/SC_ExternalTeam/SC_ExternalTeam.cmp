<aura:component implements="lightning:hasPageReference,lightning:actionOverride,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction,lightning:isUrlAddressable" access="global">
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    
    <aura:attribute name="recordId" type="Id" access="public" />
    
    <aura:attribute name="escRecordId" type="String" access="public" />
    <aura:attribute name="caseRecId" type="String" access="public" />

    <!-- Aditi - added workspace and boolean attribute isConsole for returning to the lwc-->
    <!--Aditi - also added the string attribute to store the url returned from lwc in the event ongettingtheurl -->
    <lightning:workspaceAPI aura:id="workspace"/>
    <aura:attribute name="isConsole" type="Boolean" access="public" />
    <aura:attribute name="urltoGoTo" type="String" access="public" />
    
    <c:scExternalTeamEscalation caseRecId="{!v.caseRecId}" escRecordId="{!v.escRecordId}" newInline="false" ongettingtheurl="{!c.getDatafromEvent}" oncloseSubTab="{!c.handleFilterChange}" isConsole="{!v.isConsole}"/>
</aura:component>