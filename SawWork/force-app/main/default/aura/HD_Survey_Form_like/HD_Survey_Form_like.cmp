<aura:component >
    <aura:attribute name="value" type="String" default=""/>    
    <aura:attribute name="title" type="String" default="Did you like it ?"/>    
    <!--aura:handler name="init" value="{!this}" action="{!c.facesInitAction}" /-->
	<div class="slds-form-element">
        <label class="slds-form-element__label" for="">
            <span class="slds-slider-label">
                <span class="slds-form-element__legend slds-slider-label__label">{!v.title}</span>
            </span>
        </label>
        <div style="display:block-inline">       
         <lightning:buttonIcon aura:id="like" iconName="utility:like"  size="large" iconClass=""   variant="bare" alternativeText="like" value="like" onclick="{!c.setThumb}" />
         <lightning:buttonIcon aura:id="dislike" iconName="utility:dislike"  size="large" iconClass=""     variant="bare" alternativeText="dislike" value="dislike" onclick="{!c.setThumb}"/>
         </div>
    </div>
</aura:component>