<aura:component access="global" controller="LicenseChart" implements="flexipage:availableForAllPageTypes">
    <aura:attribute name="userlist" type="List"/>
    <!--aura:handler name="init" value="{!this}" action="{!c.initAction}"/-->
    
    <!--aura:attribute name="test" type="String"/>
    <aura:handler event = "c:License_ChartToTable" action="{!c.initActionOnProfile}"/>
    Result is {!v.test}-->
    
    <!--Testing for child component call-->
    <aura:method name="myMethod" action="{!c.executeMyMethod}">
    	<aura:attribute name="param1" type="String"/>
	</aura:method>
    
    <!--method for 6 Months inactivity-->
    <aura:method name="sixmonths" action="{!c.executesixmonths}">
    	<aura:attribute name="param1" type="Integer"/>
    </aura:method>
    
    <!--method for users not owners-->
    <aura:method name="usernotowner" action="{!c.executeusernotowner}">
    	<aura:attribute name="param1" type="Integer"/>
	</aura:method>
    
    <!--methods for frequencies-->
    <aura:method name="modifications" action="{!c.executemodifications}">
    	<aura:attribute name="param1" type="Integer"/>
        <aura:attribute name="param2" type="Integer"/>
        <aura:attribute name="param3" type="Integer"/>
    </aura:method>
    <!--aura:method name="rare" action="{!c.executerare}"/>
    <aura:method name="often" action="{!c.executeoften}"/>
    <aura:method name="average" action="{!c.executeaverage}"/>
    <aura:method name="regular" action="{!c.executeregular}"/>
    <aura:method name="nerdy" action="{!c.executenerdy}"/-->
    <aura:method name="bluemoont" action="{!c.executebluemoont}">
    	<aura:attribute name="param2" type="List"/>
    </aura:method>
    <!--aura:method name="raret" action="{!c.executeraret}">
    	<aura:attribute name="param2" type="List"/>
    </aura:method>
    <aura:method name="oftent" action="{!c.executeoftent}">
    	<aura:attribute name="param2" type="List"/>
    </aura:method>
    <aura:method name="regulart" action="{!c.executeregulart}">
    	<aura:attribute name="param2" type="List"/>
    </aura:method>
    <aura:method name="nerdyt" action="{!c.executenerdyt}">
    	<aura:attribute name="param2" type="List"/>
    </aura:method-->
    
    
    <table class="slds-table slds-table_cell-buffer slds-table_bordered">
        <thead>
            <tr class="slds-line-height_reset">
                <th class="" scope="col">
                    <div class="slds-truncate" title="Name">Name</div>
                </th>
                <th class="" scope="col">
                    <div class="slds-truncate" title="Username">Username</div>
                </th>
                <th class="" scope="col">
                    <div class="slds-truncate" title="Profile">Profile</div>
                </th>
                <th class="" scope="col">
                    <div class="slds-truncate" title="Active">Active</div>
                </th>
                <th class="" scope="col">
                    <div class="slds-truncate" title="ServiceDesk Staff">ServiceDesk Staff</div>
                </th>
                <th class="" scope="col">
                    <div class="slds-truncate" title="Last Login">Last Login</div>
                </th>
            </tr>
        </thead>
        <tbody>
            <aura:iteration items="{!v.userlist}" var="lis">
            <tr class="slds-hint-parent">
                <td data-label="Name">
                    <div class="slds-truncate" title="Name" onclick="{!c.callRecord}" id="{!lis.Id}">{!lis.Name}<!--/a--></div>
                </td>
                <td data-label="Username">
                    <div class="slds-truncate" title="Username" onclick="{!c.callRecord}" id="{!lis.Id}">{!lis.Username}</div>
                </td>
                <td data-label="Profile">
                    <div class="slds-truncate" title="Profile" onclick="{!c.callRecord}" id="{!lis.Id}">{!lis.Profile.Name}</div>
                </td>
                <td data-label="IsActive">
                    <div class="slds-truncate" title="IsActive" onclick="{!c.callRecord}" id="{!lis.Id}">{!lis.IsActive}</div>
                </td>
                <td data-label="BMCServiceDesk__IsStaffUser__c">
                    <div class="slds-truncate" title="BMCServiceDesk__IsStaffUser__c" onclick="{!c.callRecord}" id="{!lis.Id}">{!lis.BMCServiceDesk__IsStaffUser__c}</div>
                </td> 
                <td data-label="LastLoginDate">
                    <div class="slds-truncate" title="LastLoginDate" onclick="{!c.callRecord}" id="{!lis.Id}">{!lis.LastLoginDate}</div>
                </td>
            </tr>
           </aura:iteration>
        </tbody>
    </table>
</aura:component>