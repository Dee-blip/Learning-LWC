<aura:component  implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickActionWithoutHeader"
access="global">
   <aura:attribute name="backgroundImageURL" type="String"/>
   <aura:handler name="init" action="{!c.onInit}" value="{!this}" />
    <!-- header start -->
    <header class="az-header">
    
        <div class="main-nav-wrapper">
            <ul class="main-nav">
                <li class="main-nav-item logo">
                    <a class="logo" style = "{! 'background-image:url(\'' + v.backgroundImageURL+ '\');'}"></a>
                </li>
            </ul>
        </div>
    </header>
    <!-- header end -->

</aura:component>