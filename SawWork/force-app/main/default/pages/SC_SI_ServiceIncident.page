<apex:page controller="SC_SI_Utility">
    
<script type="text/javascript">
  window.load=load();
  function load(){
      
  var loc ='{!getLocation}';
  
        var incNum = '{!inciName}';
  if(loc == '' && incNum != null && incNum != '')
  {
      alert('There is no such Incident Number : {!inciName}');
  }
  
  else if(loc!=''){
      
    if( incNum.includes('SI-')) {
        
        window.location='/lightning/r/SC_SI_Service_Incident__c'+loc+'/view';
        }
    else{
        
        window.location='/lightning/r/BMCServiceDesk__Incident__c'+loc+'/view';
        }
  }    
 
  }
    </script>     
   
</apex:page>