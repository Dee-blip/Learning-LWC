<apex:page standardController="Lead" recordSetVar="Records"
           extensions="LeanData_Account_Match_Controller" cache="false"  standardStylesheets="false" sidebar="false" applyBodyTag="false" docType="html-5.0">
        <html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" lang="en">
        <head>
          <meta charset="utf-8" />
          <meta http-equiv="x-ua-compatible" content="ie=edge" />
          <title>Accept Account Match</title>
          <meta name="viewport" content="width=device-width, initial-scale=1" />  
        </head>
        <body>
        
         <apex:includeLightning />
    
              
        <div id="theLead"/>
    
         <script>
           
            $Lightning.use("c:LeanData_Account_Match", function() {

                $Lightning.createComponent(

                    "c:LeanData_Account_Match_Component",

                    {
                        recordId: (({!ISBLANK(Record)}) ? null :{!Record}),
                        fromVf: "true",
                        homeURL: "{!initialURL}",
                        errorSelection: "{!errorUnselected}"
                        
                        
                    },

                    "theLead",

                 function(cmp) {
                        console.log("Component is created!");
                    });

                });

        
          </script>

            </body>
    </html>
    

    <apex:pageMessages /><br/>
    

</apex:page>