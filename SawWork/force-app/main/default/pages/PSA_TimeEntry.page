    function customRound(toRound)
    {
      if(toRound === undefined || isNaN(toRound))
      {
          toRound = 0;
      }
      var toR  = Number(toRound);
      var rounded = parseInt(toR);
      if(rounded < 10)
      {
        rounded = '0' + rounded;
      }
 
      var sixty60 = Math.round((toR - rounded) * 60.0);
      if(sixty60 < 10)
      {
        sixty60 = '0'+ sixty60;
      }
 
 
      return rounded + ' ' + sixty60;
    }
     
