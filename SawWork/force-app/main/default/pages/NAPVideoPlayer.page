<apex:page sidebar="false" showHeader="false" applyBodyTag="false" applyHtmlTag="false" standardStylesheets="false">
    <!-- SFDC-7653 - @nadesai : Redirect to APPVideoPlayer page along with Page parameters-->
    <script type="text/javascript">
        window.onload = function () {
            var redirectionURL = "{!$CurrentPage.URL}";
            redirectionURL = redirectionURL.replace(/NAPVideoPlayer/i, "APPVideoPlayer");//SFDC-7680
            if ("{!$CurrentPage.URL}" != redirectionURL) { //SFDC-7681
                window.location = redirectionURL;
            }
        }
    </script>
</apex:page>