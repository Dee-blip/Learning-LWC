<apex:page id="manage_orders_page">
  <script type="text/javascript" language="javascript">
    function view_orders() {
      window.parent.location = "{!$label.POA_Documents_Tab_Link}";
    }
    function cancel_orders() {
      window.parent.location = "{!$label.PartnerOrderCancellationNAPLink}";
    }
  </script>
  <apex:pageBlock >
    <h1>Manage Orders</h1>
    <apex:pageBlockButtons location="bottom">
      <input type="submit" onclick="view_orders();" value="View Partner Orders" />
      <input type="submit" onclick="cancel_orders();" value="Cancel Partner Orders" />
    </apex:pageBlockButtons>
  </apex:pageBlock>
</apex:page>