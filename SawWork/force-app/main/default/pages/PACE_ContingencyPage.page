<apex:page standardController="PACE_Review_Calendar__c" extensions="PACE_ContingencyPageController">
   
  <apex:form >
      <apex:pageBlock >
     <apex:outputText rendered="{!isListNull}">No Contingency records available </apex:outputText>
      <apex:pageblockSection columns="1" collapsible="true" >
                     <apex:pageBlockTable value="{!contingencyRecordList}" var="h" rendered="{!!isListNull}">
                        <apex:column style="width:20%" headerValue="Name">
                             <apex:commandLink value="{!h.Name}" onclick="window.open('{!$Organization.Division}/{!h.Id}')"/> 
                        </apex:column>
                        <apex:column style="width:10%" headerValue="Status"  value="{!h.Status__c }"/>
                        <apex:column style="width:10%" headerValue="Due Date"  value="{!h.Due_Date__c}"/>
                      <!--  <apex:column style="width:25%" headerValue="Concept">
                            <apex:commandLink value="{!h.Concept__r.Name}" onclick="window.open('{!$Organization.Division}/{!h.Concept__c}')"/>
                        </apex:column>-->
                        <apex:column style="width:25%" headerValue="Phase">
                            <apex:commandLink value="{!h.Phase__r.Name}" onclick="window.open('{!$Organization.Division}/{!h.Phase__c}')"/>
                        </apex:column>
                        <apex:column style="width:10%" headerValue="Closed Date"  value="{!h.Closed_Date__c}"/>
                     </apex:pageBlockTable>
      </apex:pageblockSection>
     
      </apex:pageBlock> 
     
  </apex:form> 
</apex:page>