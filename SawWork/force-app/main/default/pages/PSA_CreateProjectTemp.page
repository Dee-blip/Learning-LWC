<apex:page id="PSA_CreateProjectTemp" standardController="pse__Proj__c" extensions="PSA_CreateProjectTempController" action="{! navigateToCreatePage}">
    
    <apex:slds />
    <apex:includeScript value="{!$Resource.PSA_Javascript}"/>
	<apex:includeScript value="{!$Resource.JQuery_New}"/>

    <script type="text/javascript">
          
             $ = jQuery.noConflict(); 
             $( document ).ready(function() {
                 $('#status').show();
                  setTimeout(function() {
                  		$('#status').hide();
                  }, 1000);             

            });

    </script>
    
    <apex:pageMessages />
    <div  id="status" style="width: 100%;height: 100%;display:none;" class="slds-scope slds-spinner_container" >
        <div role="status" class="slds-spinner slds-spinner--medium slds-spinner--brand"  >
            <span class="slds-assistive-text">Loading</span>
            <div class="slds-spinner__dot-a"></div>
            <div class="slds-spinner__dot-b"></div>
        </div>
        
    </div>
    
    
</apex:page>