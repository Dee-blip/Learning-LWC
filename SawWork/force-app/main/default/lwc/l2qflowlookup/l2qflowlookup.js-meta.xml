<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata" fqn="fileUploadLWCdemo">
    <apiVersion>49.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>L2Q Flow Lookup </masterLabel>
    <description>This is a demo component.</description>
    <targets>
        <target>lightning__AppPage</target>
        <target>lightning__RecordPage</target>
        <target>lightning__HomePage</target>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="flowrecname" type="String" label="Selected Record Name" description="Select Record Name"/>
            <property name="flowrecid" type="String" label="Selected Record Id" description="Selected Record Id"/>
            <property name="flowrequired" type="Boolean" label="Required ?" description="Required ?" role="inputOnly"/>
            <property name="iconname" type="String" label="Record Icon" description="Record Icon" role="inputOnly"/>
            <property name="objectName" type="String" label="Search Object API Name" description="Search Object API Name" role="inputOnly" required="true" default="User"/>
            <property name="searchfield" type="String" label="Search Field API Name" description="Only Compatible Fields Allowed" role="inputOnly"/>
            <property name="recordLimit" type="String" label="Search Limit" description="Search Limit .i.e 10,20,30 etc" role="inputOnly"/>
            <property name="placeholderText" type="String" label="Placeholder" description="" role="inputOnly"/>
            <property name="whereClause" type="String" label="SOQL Filter Clause" description="and status__c = 'Pending'" role="inputOnly"/>
            <property name="minSearchlen" type="String" label="Minimum Character for Search" description="Minimum Search Length" role="inputOnly"/>
            <property name="maxtimedelay" type="String" label="Maximum Time Delay::" description="Maximum time delay" role="inputOnly"/>
            <property name="fieldtodisplay" type="String" label="API Name of Secondary Field" description="" role="inputOnly"/>
            <property name="cssStyle" type="String" label="Template CSS" description="Template CSS i.e width:75%" role="inputOnly"/>
            <property name="errorMessageonrequired" type="String" label="Required Field Error Message" description="" role="inputOnly"/>
            <property name="fieldlabel" type="String" label="Field Label" description="" role="inputOnly"/>
            <property name="fieldlabelcss" type="String" label="Field Label CSS" description="Additional CSS for Field Label" role="inputOnly"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>