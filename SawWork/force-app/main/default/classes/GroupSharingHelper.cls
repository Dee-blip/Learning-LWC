    //added by Ankita 4/28/2010 for dynamic SOQL so portals are not required
    public static String join(List<Id> values, String separator, String enclosedBy) {
        // Handle null values
        if ((values == null) || (values.size() == 0)) {
            return '';
        }
        if (separator == null) {
            separator = '';
        }
        if (enclosedBy == null) {
            enclosedBy = '';
        }
        
        String result = '';
        for (Id value : values) {
            if (value == null) {
                continue;
            }
            result += (enclosedBy + value + enclosedBy + separator);
        }
        // Chop off trailing separator
        result = result.substring(0,result.length() - separator.length());
        return result;
    }
    
    public static String join(Set<Id> values, String separator, String enclosedBy) {
        return join(new List<Id> (values), separator, enclosedBy);
    }
