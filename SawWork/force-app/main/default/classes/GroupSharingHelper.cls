    public static void getAccParentMap(List<Id> accIds, Boolean shareWithThisAcc){
        
        Map<Id,Account> aMap = new Map<Id, Account>();
        if(accIds != null && accIds.size()>0){
            if(!ISTEST){
                aMap.putAll([select id, ParentId, Parent.ParentId, Parent.Parent.ParentId, Parent.Parent.Parent.ParentId, 
                                                    Parent.Parent.Parent.parent.ParentId, Parent.Parent.Parent.parent.Parent.ParentId
                                                    from Account where Id in :accIds]); //new Account(Id = accId);
            }else{
                aMap.putAll([select id, ParentId, Parent.ParentId, Parent.Parent.ParentId, Parent.Parent.Parent.ParentId, 
                                                    Parent.Parent.Parent.parent.ParentId, Parent.Parent.Parent.parent.Parent.ParentId
                                                    from Account where Id in :accIds limit 10]); //new Account(Id = accId);
            }
        for(Account a : aMap.values()){
        	String parents = '';
            if(a.ParentId != null){
                parents = a.ParentId;            
            }
            Account parent = a.Parent;  
            if(parent != null){      
                while(parent.ParentId != null){
                    parents = parents + ',' + parent.ParentId;
                    if(parent.Parent != null){
                        parent = parent.Parent;
                    }
                }
            }
            if(shareWithThisAcc){
                if(parents.equals('')){
                    parents = a.Id;
                }else{
                    parents = parents + ',' + a.Id;
                }
            }
            List<Id> pIds = new List<Id>();
            if(!parents.equals('')){
                parentAccMap.put(a.Id, parents);
                pIds = parents.split(',');
            }
            parentIds.addAll(pids);
        }
        }
    }
    
    public static void getOldAccParentMap(Map<Id, String> accIds, Boolean shareWithThisAcc){
        String parents = '';
        Map<Id, String> parentsMap = new Map<Id,String>();
        Map<Id,Account> aMap = new Map<Id,Account>();
        Set<Id> ids = new Set<Id>();
        for(String s : accIds.values()){
            for(String x : s.split(',')){
                ids.add(x);
            }
        }
        if (ids.size()>0)
        {
            if(!ISTEST){
                aMap.putAll([select id, ParentId, Parent.ParentId, Parent.Parent.ParentId, Parent.Parent.Parent.ParentId, 
                                                    Parent.Parent.Parent.parent.ParentId, Parent.Parent.Parent.parent.Parent.ParentId
                                                    from Account where Id in :ids]); //where Id in :accIds.values() new Account(Id = accId);
            }else{
                aMap.putAll([select id, ParentId, Parent.ParentId, Parent.Parent.ParentId, Parent.Parent.Parent.ParentId, 
                                                        Parent.Parent.Parent.parent.ParentId, Parent.Parent.Parent.parent.Parent.ParentId
                                                        from Account where Id in :Ids limit 10]); //where Id in :accIds.values() new Account(Id = accId);
            }
        }
        for(Account a : aMap.values()){
            parents = a.Id;              
            Account parent = a; 
            if(parent != null){      
                while(parent.ParentId != null){
                    parents = parents + ',' + parent.ParentId;
                    if(parent.Parent != null){
                        parent = parent.Parent;
                    }
                }
            }
            if(shareWithThisAcc){
                if(parents.equals('')){
                    parents = a.Id;
                }else{
                    parents = parents + ',' + a.Id;
                }
            }
            parentsMap.put(a.Id, parents);
            List<id> pIds = parents.split(',');
            oldParentIds.addAll(pids);
        }
        for(Id id : accIds.keySet()){
            String x = accIds.get(id);
            for(String x1 : x.split(',')){
                oldParentAccMap.put(id,parentsMap.get(x1));
            }
        }
    }

